fn parse(json: string, options: JsonParseOptions): table {
	tokenizer: Tokenizer = Tokenizer.new(json);
	tokens: Token[] = tokenizer.tokenize();
	parser: Parser = Parser.new(tokens);
	return parser.parse(options);
}

